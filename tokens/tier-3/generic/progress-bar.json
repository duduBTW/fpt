{
  "progress-bar": {
    "color": {
      "background": { "$value": "{colors.gray.200}" },
      "content": { "$value": "{colors.gray.500}" },
      "progress": { "$value": "{colors.red.500}" }
    },
    "spacing": {
      "circular": {
        "size": { "$value": 32 },
        "strokeSize": { "$value": 3 }
      },
      "linear": {
        "height": { "$value": "{spacing.2}" }
      }
    },
    "rectangle": {
      "radius": {
        "$type": "dimension",
        "$value": "{radius.default}"
      }
    }
  }
}
